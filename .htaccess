AddType application/x-httpd-php81 .php

<IfModule mod_rewrite.c>
    RewriteEngine On

    RewriteCond %{SERVER_PORT} 80
    RewriteRule ^(.*)$ https://docs.tuliacms.org/$1 [R,L]

    RewriteRule ^/?docs/(.*)$ public/docs/$1 [NC,L,QSA,END]
    RewriteRule ^(.*)$ public/index.php [NC,L,QSA,END]
</IfModule>

<IfModule !mod_rewrite.c>
    deny from all
</IfModule>
